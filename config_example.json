{
   "get_ip":"https://itkit.com.ua/telbot/getip2.php", --- Путь к сайту которого будет получатся IP для отправки
   "default_ip":"127.0.0.1",  --- Если ip не был получен с сайта, отчеты будут отправлятся сюда
   "restaurant_id":1883,  --- id ресторана в формате integer (целое число)
   "sleep_parse_sec":120, --- Время сна каждого парсинга файлов принтера в секундах
   "sleep_send_sec":10,  --- Время сна каждой проверки наличия файлов для отправки
   "printer_dir":"/Users/mac/Downloads/Printers", --- Дирректория где искать отчеты, на Windows путь через / (формат Unix)
   "include_markers": true, --- Включать ли маркер начала и конца в текст отправляемого сообщения
   "markers": [ --- Маркеры начала и конца отчета в виде регулярных выражений,
      [
         "Бирхаус Пушк сменный отчет",
         "#начало-отчета-0",
         "#конец-отчета-0"
      ],
      [
         "Бирхаус Пушк расход блюд",
         "#начало-отчета-1",
         "#конец-отчета-1"
      ]
   ]
}